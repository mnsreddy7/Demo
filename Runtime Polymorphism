class ABC1{
    public void myMethod(){
        System.out.println("Overridden Method");
    }
}
 class XYZ extends ABC1{

    public void myMethod(){
        System.out.println("Overriding Method");
    }
    public static void main(String args[]){
        ABC1 obj = new XYZ();
        obj.myMethod();
    }
}
